var bool a;
var bool b;

main {

  if (StdLib.max(41, 42) == 42) {

    a = false;
    b = true;

    assert (!a && b);
    assert (a || b);
    assert !(a && !b);
    assert !(!a && !b);
    assert !(a && b);

    printf("Tous les tests passent !\n");

    assert a && b;

    printf("Ou pas...\n");

  }

  return 0;
}

