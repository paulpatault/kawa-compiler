class StdLib {

  method void constructor() { }

  @{static}
  method int max(int a, int b) {
    if (a > b) {
      return a;
    } else {
      return b;
    }
  }

  @{static}
  method int min(int a, int b) {
    if (a < b) {
      return a;
    } else {
      return b;
    }
  }

  @{static}
  method int pow(int x, int n) {
    if (n == 0) {
      return 1;
    } else {
      return x * StdLib.pow(x, n - 1);
    }
  }

}

