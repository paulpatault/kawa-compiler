var int i;


class StdLib {

  method void constructor() { }

  @{not_optim, static}
  method int max(int a, int b) {
    if (a > b) {
      return a;
    } else {
      return b;
    }
  }

  @{not_optim, static}
  method int min(int a, int b) {
    if (a < b) {
      return a;
    } else {
      return b;
    }
  }

  @{not_optim, static}
  method int modulo(int x, int n) {
    if (x == n) {
      return 0;
    } else {
      if (x < n) {
        return x;
      } else {
        return StdLib.modulo(x - n, n);
      }
    }
  }

  @{not_optim, static}
  method int pow(int x, int n) {
    var int r;
    r = 0;
    putchar("n=");
    putchar(n);
    putchar(" ");
    if (n == 0) {
      return 1;
    } else {
      r = StdLib.pow(x, n/2);
      r = r * r;
      if (StdLib.modulo(n, 2) == 1) {
        putchar("r2=");
        putchar(r*r);
        putchar(" ");
        return r * r;
      } else {
        putchar("r=");
        putchar(r);
        putchar(" ");
        return r;
      }
    }
  }

}

