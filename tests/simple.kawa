var P4 p4;

class P1 {
  attribute int x;
  method void constructor(int x) { this.x = x; }
}

class P2 {
  attribute P1 p1;
  method void constructor(int x) { this.p1 = new P1(x); }
}

class P3 {
  attribute P2 p2;
  method void constructor(int x) { this.p2 = new P2(x); }
}

class P4 {
  attribute P3 p3;
  method void constructor(int x) { this.p3 = new P3(x); }
}

