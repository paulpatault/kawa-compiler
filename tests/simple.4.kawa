var point2d p2d;
var point3d p3d;

class point2d {

  attribute int x;
  attribute int y;

  method void constructor(int px, int py) {
    this.x = px;
    this.y = py;
  }

  method int sum() {
    return this.x + this.y;
  }

}

class point3d extends point2d {

  attribute int z;

  method void constructor(int px, int py, int pz) {
    super(px, py);
    /* this.x = px;
    this.y = py; */
    this.z = pz;
  }

}

main {
  /* p2d = new point2d(4, 2); */
  p3d = new point3d(1, 2, 3);
  putchar(p3d.x);
  putchar(" ");
  putchar(p3d.y);
  putchar(" ");
  putchar(p3d.z);
  return 0;
}
