var bool a;
var bool b;

main {

  a = false;
  b = true;

  if(!a && b) {
    printf("ok\n");
  }

  if (a || b) {
    printf("ok\n");
  }

  if (a || !b) {
    printf("not ok\n");
  }

  if (!a || !b) {
    printf("ok\n");
  }

  if (!(a && !b)) {
    printf("ok\n");
  }

  if (!(!a && !b)) {
    printf("ok\n");
  }

  if (!(a && b)) {
    printf("ok\n");
  }

  return 0;
}
